@Table(value="Sm_fapiao")
public class Sm_fapiaoEntity{
	 @Column
	private int reco_no;
	 @Column
	private String fp_mc;
	 @Column
	private double fp_shlv;
	 @Column
	private short fp_def;
	 @Column
	private String XiaoShui_KeMu_Dm;
	 @Column
	private String JinShui_KeMu_Dm;
	public void setReco_no(int reco_no){
	this.reco_no=reco_no;
	}
	public int getReco_no(){
		return reco_no;
	}
	public void setFp_mc(String fp_mc){
	this.fp_mc=fp_mc;
	}
	public String getFp_mc(){
		return fp_mc;
	}
	public void setFp_shlv(double fp_shlv){
	this.fp_shlv=fp_shlv;
	}
	public double getFp_shlv(){
		return fp_shlv;
	}
	public void setFp_def(short fp_def){
	this.fp_def=fp_def;
	}
	public short getFp_def(){
		return fp_def;
	}
	public void setXiaoShui_KeMu_Dm(String XiaoShui_KeMu_Dm){
	this.XiaoShui_KeMu_Dm=XiaoShui_KeMu_Dm;
	}
	public String getXiaoShui_KeMu_Dm(){
		return XiaoShui_KeMu_Dm;
	}
	public void setJinShui_KeMu_Dm(String JinShui_KeMu_Dm){
	this.JinShui_KeMu_Dm=JinShui_KeMu_Dm;
	}
	public String getJinShui_KeMu_Dm(){
		return JinShui_KeMu_Dm;
	}
}

